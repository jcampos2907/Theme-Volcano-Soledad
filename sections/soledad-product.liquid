<section
  id="MainProduct-{{ section.id }}"
  class="page-width section-{{ section.id }}-padding"
  data-section="{{ section.id }}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-rte.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {{ 'image-picker.css' | asset_url | stylesheet_tag }}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}
  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'populate_countries.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'multi-step-pruebas.js' | asset_url }}" defer="defer"></script>
  
{% comment %}   <script src="{{ 'image-picker.js' | asset_url }}" defer="defer"></script> {% endcomment %}
  
  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
      {% render 'product-media-gallery', variant_images: variant_images %}
    </div>
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
      <product-info
        id="ProductInfo-{{ section.id }}"
        data-section="{{ section.id }}"
        data-url="{{ product.url }}"
        class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
      >
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              {% render block %}
            {%- when 'text' -%}
              <p
                class="product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                {{ block.shopify_attributes }}
              >
                {{- block.settings.text -}}
              </p>
            {%- when 'title' -%}
              <div class="product__title" {{ block.shopify_attributes }}>
                <h1>{{ product.title | escape }}</h1>
                <a href="{{ product.url }}" class="product__title">
                  <h2 class="h1">
                    {{ product.title | escape }}
                  </h2>
                </a>
              </div>
            {%- when 'price' -%}
              <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                {%- render 'price',
                  product: product,
                  use_variant: true,
                  show_badges: true,
                  price_class: 'price--large'
                -%}
              </div>
              {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                <div class="product__tax caption rte">
                  {%- if shop.taxes_included -%}
                    {{ 'products.product.include_taxes' | t }}
                  {%- endif -%}
                  {%- if shop.shipping_policy.body != blank -%}
                    {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- endif -%}
                </div>
              {%- endif -%}
              <div {{ block.shopify_attributes }}>

            <div style = 'padding: 40px 40px;' >{% render 'soledad-logo_white' %}</div>
           {%- if product.description != blank -%}
              <div class="subtitle product__description rte quick-add-hidden" {{ block.shopify_attributes }}>
                {{ product.description }}
              </div>
            {%- endif -%}
                {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                  <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                  <div class="stepper-wrapper">
                    <div class="stepper-item text-body" onclick = 'indicatorNext(0)' style = 'cursor:pointer' >
                      <div class="step-counter" >1</div>
                      <div class="step-name">Participante 1</div>
                    </div>
                    <div class="stepper-item text-body" onclick = 'indicatorNext(1)' style = 'cursor:pointer'>
                      <div class="step-counter" >2</div>
                      <div class="step-name">Participante 2</div>
                    </div>
                    <div class="stepper-item text-body" onclick = 'indicatorNext(2)' style = 'cursor:pointer'>
                      <div class="step-counter">3</div>
                      <div class="step-name">Datos del Paquete</div>
                    </div>
                  </div>
                  <div class='tab' id="Participante 1">
                    <div class = 'step_section subtitle'>Datos del participante</div>
                    <div class='columns identificacion'>
                        <p class="line-item-property__field">
                            <label for="nacionalidad">Nacionalidad:</label>
                            <select required class='required nacionalidad' id="nacionalidad" name="properties[Nacionalidad]">
                                <option value="" disabled selected>--NACIONALIDAD--</option>
                            </select>
                        </p>
                        <p class="line-item-property__field">
                            <label cedula='true' for="identificacion_corredor">Numero de Cédula : </label>
                            <input required disabled class="required mask identificacion_corredor" id="identificacion_corredor" type="text"
                                name="properties[Numero de identificacion]">
                        </p>
                    </div>
                    <p class="line-item-property__field">
                        <label for="nombre">Primer nombre</label>
                        <input required class="required nombre" idcontrol='identificacion_corredor' id="nombre" type="text"
                            name="properties[Nombre]">
                    </p>
                    <div class='columns'>
                        <p class="line-item-property__field">
                            <label for="apellido">Apellido</label>
                            <input required class="required apellido" idcontrol='identificacion_corredor' id="apellido" type="text"
                                name="properties[Apellido]">
                        </p>
                        <p class="line-item-property__field">
                            <label for="segundo_apellido">Segundo apellido</label>
                            <input required class="required segundo_apellido" idcontrol='identificacion_corredor' id="segundo_apellido"
                                type="text" name="properties[Segundo apellido]">
                        </p>
                    <span class = 'caption cedula__helper' style = 'padding-bottom:2rem'>¿No es tu nombre? Aseguráte de haber escrito tu cédula correctamente</span>
                    </div>
                    <div class='columns'>
                        <p class="line-item-property__field telefono">
                            <label for="codigo_telefono_corredor">Telefono del corredor:</label>
                            <span class='phone'>
                                <span class='col-1'>
                                    <select class='codigo_telefono' id="codigo_telefono_corredor" is="ms-dropdown">
                                        <option selected disabled data-style="background-image: url('https://flagcdn.com/w320/cr.png')"
                                        value="{&quot;code&quot;:&quot; +506&quot;,&quot;countrycode&quot;:&quot;CR&quot;,&quot;image&quot;:&quot;https://flagcdn.com/w320/cr.png&quot;}">
                                        Costa Rica</option>
                                    </select>
                                </span>
                
                                <span class='col-2'>
                                    <input required class="required mask" type="tel" id="telefono_corredor"
                                        name="properties[Telefono del corredor]">
                                </span>
                            </span>
                        </p>
{% comment %}                   <p>Date: <input style = 'z-index = 10' type="text" id="datepicker"></p> {% endcomment %}
                        <p class="line-item-property__field">
                            <label for="fecha_nacimiento">Fecha de Nacimiento:</label>
                  <input readonly required class='required fecha_nacimiento mask' type="text" id="fecha_nacimiento" name="properties[Fecha de nacimiento]">
                  
{% comment %}                             <input required class='required fecha_nacimiento' type="date" id="fecha_nacimiento" name="properties[Fecha de nacimiento]"
                                min="1943-09-03" max="2005-09-03"> {% endcomment %}
                        </p>
                    </div>
                    <div>
                        <p class="line-item-property__field">
                            <label for="correo_electronico">Correo electronico:</label>
                            <input required class="required" type="email" id="correo_electronico" name="properties[Correo electronico]">
                        </p>
                        <p class="line-item-property__field">
                            <label for="genero">Genero:</label>
                            <select required class='genero required' id="genero" name="properties[Genero]"
                                data-class="avatar">
                                <option value="" disabled selected>--GENERO--</option>
                                <option value="Hombre">Hombre</option>
                                <option value="Mujer">Mujer</option>
                            </select>
                        </p>
                    </div>
                
                    <p class="line-item-property__field">
                        <label>Pertenece a algun equipo élite o master?</label><br>
                        <input type="radio" name="properties[Pertenece a algun equipo élite o master?]" value="Si"> <span>Si</span><br>
                        <input type="radio" checked="checked" name="properties[Pertenece a algun equipo élite o master?]" value="No">
                        <span>No</span><br>
                    </p>
                    <p id='container_equipo' hidden>
                        <label for="equipo">Nombre del equipo:</label>
                        <select class='select_normal' id="equipo" name="properties[Equipo]">
                            <option value="" disabled selected>--EQUIPO--</option>
                            <option value="Team Colono Bikestation Clips">Team Colono Bikestation Clips</option>
                            <option value="Move CC">Move CC</option>
                            <option value="Bikestation Riders">Bikestation Riders</option>
                            <option value="Bike Ride Costa Rica">Bike Ride Costa Rica</option>        
                            <option value="Pedregal- 7C - Economy">Pedregal- 7C - Economy</option>
                            <option value="MET">MET (Milla Extra Team) </option>
                            <option value="Mmr-Sportivestore">Mmr-Sportivestore</option>
                            <option value="Morning Riders">Morning Riders</option>
                            <option value="CRPRO">CRPRO</option>
                            <option value="CBZ Asfaltos- Cape Healthcare">CBZ Asfaltos- Cape Healthcare</option>
                            <option value="Bobby´s Cycling Team">Bobby´s Cycling Team</option>
                            <option value="Team Innovabikes">Team Innovabikes</option>
                            <option value="Cité De Velo">Cité De Velo</option>
                            <option value="Mingo Cycling">Mingo Cycling</option>
                            <option value="Team Gocha">Team Gocha</option>
                            <option value="Kairos">Kairos</option>
                            <option value="BitCycling">BitCycling</option>
                            <option value="Ciclo Víctor Vargas">Ciclo Víctor Vargas</option>  
                        </select>
                      <a style='color:white; padding-top:1rem; opacity:0.75' href="mailto:info@volcano100cr.com?subject=Consulta">¿Sos el entrenador de un equipo y no lo ves? Conatctános para incluirlos en nuestra base</a>
                    </p>
                    <p class="line-item-property__field">
                        <label>Tiene licencia FECOCI anual?</label><br>
                        <input type="radio" checked="checked" name="properties[Tiene licencia FECOCI anual?]" value="Si" idcontrol='identificacion_corredor'> <span>Si</span><br>
                        <input type="radio" name="properties[Tiene licencia FECOCI anual?]" value="No" idcontrol='identificacion_corredor'><span>No</span><br>
                    </p>
                    <span class = 'caption' style = 'padding-bottom:1rem'>La licencia de un dia tiene un costo adicional de $15 por persona</span>
                  
                    <div id='container_licencia'>
                        <div class='columns'>
                            <p class="line-item-property__field">
                                <label for="num_licencia">Numero de licencia anual:</label>
                                <input required class = 'num_licencia required' type='text' id='num_licencia' name="properties[Numero de Licencia]" idcontrol='identificacion_corredor'>
                            </p>
                            <p class="line-item-property__field">
                                <label for="foto_licencia">Foto licencia de licencia anual:</label>
                                <input required class = 'required' type='file' id='foto_licencia' name="properties[Foto Licencia]" accept="image/png, image/jpeg">
                            </p>
                            <span class='caption' style = 'padding-bottom:1rem'>Por favor indicar el UCI ID</span>
                        </div>
                    </div>
                    <h1 class = 'step_section'>Datos del paquete</h1>
                    <div class='columns'>
                        <p class="line-item-property__field">
                            <label for="talla_jersey">Talla del Jersey:</label>
                            <select required class='select_normal required jersey' id="talla_jersey" name="properties[Talla del Jersey]">
                                <option value="" disabled selected>--Talla--</option>
                                <option value="XS">XS</option>
                                <option value="S">S</option>
                                <option value="M">M</option>
                                <option value="L">L</option>
                                <option value="XL">XL</option>
                                <option value="XXL">XXL</option>
                                <option value="XXXL">XXXL</option>
                            </select>
                        </p>
                        <p class="line-item-property__field">
                            <label for="talla_t_shirt">Talla del T-Shirt:</label>
                            <select required class='select_normal required' id="talla_t_shirt" name="properties[Talla del T-Shirt]">
                                <option value="" disabled selected>--Talla--</option>
                                <option value="XS">XS</option>
                                <option value="S">S</option>
                                <option value="M">M</option>
                                <option value="L">L</option>
                                <option value="XL">XL</option>
                                <option value="XXL">XXL</option>
                                <option value="XXXL">XXXL</option>
                            </select>
                        </p>
                    </div>
                    <div class = "modalContainer">
                      <span class = "modalBtn" style='cursor: pointer;text-decoration: underline;'>Ver guia de tallas del jersey</span>
                      <!-- The Modal -->
                      <div class="modal">
                          <!-- Modal content -->
                          <div class="modal-content">
                              <span class="close">&times;</span>
                              <img style="width:100%" src="{{ 'tablaprueba.png' | file_img_url: '1500x1500' }}" />
                          </div>
                      </div>  
                    </div>
                
                    <h1 class = 'step_section'>Informacion medica</h1>
                    <div class='columns'>
                
                        <p class="line-item-property__field">
                            <label for="tipo_sangre">Tipo de sangre:</label>
                            <select required class='select_normal required' id="tipo_sangre" name="properties[Tipo de sangre]">
                                <option value="" disabled selected>--Sangre--</option>
                                <option value="A+">A+</option>
                                <option value="A-">A-</option>
                                <option value="B+">B+</option>
                                <option value="B-">B-</option>
                                <option value="AB+">AB+</option>
                                <option value="AB-">AB-</option>
                                <option value="O+">O+</option>
                                <option value="O-">O-</option>
                            </select>
                        </p>
                        <p class="line-item-property__field">
                            <label for="lateralidad">Lateralidad:</label>
                            <select required class='select_normal required' id="lateralidad" name="properties[Lateralidad]">
                                <option value="" disabled selected>--LATERALIDAD--</option>
                                <option value="Izquierda">Izquierda</option>
                                <option value="Derecha">Derecha</option>
                            </select>
                        </p>
                    </div>
                    <p class="line-item-property__field">
                        <label for="consideraciones_medicas">¿Alguna otra consideración médica importante?</label>
                        <textarea id="consideraciones_medicas"
                            name="properties[¿Alguna otra consideración médica importante?]"></textarea>
                    </p>
                
                    <h1 class = 'step_section'>Contacto de emergencia</h1>
                    <div class='columns'>
                        <p class="line-item-property__field">
                            <label for="nombre__contacto_emergencia">Nombre del contacto de emergencia</label>
                            <input required class="required nombre" idcontrol='identificacion__contacto_emergencia'
                                id="nombre__contacto_emergencia" type="text" name="properties[Nombre del contacto de emergencia]">
                        </p>
                        <p class="line-item-property__field">
                            <label for="apellidos__contacto_emergencia">Apellidos del contacto de emergencia</label>
                            <input required class="required apellido" idcontrol='identificacion__contacto_emergencia'
                                id="apellidos__contacto_emergencia" type="text" name="properties[Apellidos del contacto de emergencia]">
                        </p>
                    </div>
                    <div class='columns identificacion'>
                        <p class="line-item-property__field">
                            <label for="nacionalidad__contacto_emergencia">Nacionalidad:</label>
                            <select required class='required nacionalidad' id="nacionalidad__contacto_emergencia"
                                name="properties[Nacionalidad del contacto de emergencia]">
                                <option value="" disabled selected>--NACIONALIDAD--</option>
                            </select>
                        </p>
                        <p class="line-item-property__field"> <label cedula='true' for="identificacion__contacto_emergencia">Numero de
                            Cédula : </label>
                            <input required disabled class="required mask" id="identificacion__contacto_emergencia" type="text"
                                name="properties[Identificacion del contacto de emergencia]">
                        </p>
                    </div>
                    <p class="line-item-property__field telefono">
                        <label for="codigo_telefono_contacto_emergencia">Telefono del contacto de emergencia:</label>
                        <span class='phone'>
                            <span class='col-1'>
                                <select class='codigo_telefono' id="codigo_telefono_contacto_emergencia" is="ms-dropdown">
                                    <option selected disabled data-style="background-image: url('https://flagcdn.com/w320/cr.png')"
                                        value="{&quot;code&quot;:&quot; +506&quot;,&quot;countrycode&quot;:&quot;CR&quot;,&quot;image&quot;:&quot;https://flagcdn.com/w320/cr.png&quot;}">
                                        Costa Rica</option>
                                </select>
                            </span>
            
                            <span class='col-2'>
                                <input required class="required mask" type="tel" id="telefono_contacto_emergencia"
                                    name="properties[Telefono del contacto de emergencia]">
                            </span>
                        </span>
                    </p>
                    <p class="line-item-property__field">
                        <label for="confirmacion_datos">
                          <input required class = 'required' type="checkbox" id="confirmacion_datos" value="Confirmo">
                          Confirmo que todos mis datos son correctos y que he leido y acepto los Terminos y Condiciones y el Reglamento de Carrera
                        </label>
                        <div class = "modalContainer">
                          <span class = "modalBtn" style='cursor: pointer;text-decoration: underline;'>Ver Terminos y Condiciones</span>
                          <!-- The Modal -->
                          <div class="modal">
                              <!-- Modal content -->
                              <div class="modal-content">
                                  <span class="close">&times;</span>
                  <span>{{ product.metafields.custom.terms_of_service }}</span>
                              </div>
                          </div>  
                        </div>
                        <div class = "modalContainer">
                          <span class = "modalBtn" style='cursor: pointer;text-decoration: underline;'>Ver Reglamento de Carrera</span>
                          <!-- The Modal -->
                          <div class="modal">
                              <!-- Modal content -->
                              <div class="modal-content">
                                  <span class="close">&times;</span>
                                  <span>{{ product.metafields.custom.race_rules }}</span>
                              </div>
                          </div>  
                        </div>
                    </p>
                </div>
                <div class='tab' id="Participante 2">
                    <div class='columns identificacion'>
                        <p class="line-item-property__field">
                            <label for="nacionalidad_2">Nacionalidad:</label>
                            <select required class='required nacionalidad' id="nacionalidad_2"
                                name="properties[Nacionalidad  (Segundo Participante) ]">
                                <option value="" disabled selected>--NACIONALIDAD--</option>
                            </select>
                        </p>
                        <p class="line-item-property__field"> <label cedula='true' for="identificacion_corredor_2">Numero de Cédula :
                            </label>
                            <input required disabled class="required mask identificacion_corredor" id="identificacion_corredor_2" type="text"
                                name="properties[Numero de identificacion  (Segundo Participante) ]">
                        </p>
                    </div>
                    <p class="line-item-property__field">
                        <label for="nombre_2">Primer nombre</label>
                        <input required class="required nombre" idcontrol='identificacion_corredor_2' id="nombre_2" type="text"
                            name="properties[Nombre  (Segundo Participante) ]">
                    <div id="cname" class="emsg"></div>
                    </p>
                    <div class='columns'>
                        <p class="line-item-property__field">
                            <label for="apellido_2">Apellido</label>
                            <input required class="required apellido" idcontrol='identificacion_corredor_2' id="apellido_2" type="text"
                                name="properties[Apellido  (Segundo Participante) ]">
                        </p>
                        <p class="line-item-property__field">
                            <label for="segundo_apellido_2">Segundo apellido</label>
                            <input required class="required segundo_apellido" idcontrol='identificacion_corredor_2'
                                id="segundo_apellido_2" type="text" name="properties[Segundo apellido  (Segundo Participante) ]">
                        </p>
                    </div>
                    <span style = 'padding-bottom:1rem'>¿No es tu nombre? Aseguráte de que escribiste bien tu cédula</span>
                    <div class='columns'>
                        <p class="line-item-property__field telefono">
                            <label for="codigo_telefono_corredor_2">Telefono del corredor:</label>
                            <span class='phone'>
                                <span class='col-1'>
                                    <select class='codigo_telefono' id="codigo_telefono_corredor_2" is="ms-dropdown">
                                        <option selected disabled data-style="background-image: url('https://flagcdn.com/w320/cr.png')"
                                        value="{&quot;code&quot;:&quot; +506&quot;,&quot;countrycode&quot;:&quot;CR&quot;,&quot;image&quot;:&quot;https://flagcdn.com/w320/cr.png&quot;}">
                                        Costa Rica</option>
                                    </select>
                                </span>
                
                                <span class='col-2'>
                                    <input required class="required mask" type="tel" id="telefono_corredor_2"
                                        name="properties[Telefono del corredor (Segundo Participante]">
                                </span>
                            </span>
                        </p>
                        <p class="line-item-property__field">
                            <label for="fecha_nacimiento_2">Fecha de Nacimiento:</label>
                  <input readonly required class='required fecha_nacimiento mask' type="text" id="fecha_nacimiento_2" name="properties[Fecha de nacimiento (Segundo Participante) ]">
                  
{% comment %}                             <input required class='required fecha_nacimiento mask' type="date" id="fecha_nacimiento_2"
                                name="properties[Fecha de nacimiento  (Segundo Participante) ]" min="1943-09-03" max="2005-09-03"> {% endcomment %}
                        </p>
                    </div>
                    <p class="line-item-property__field">
                        <label for="correo_electronico_2">Correo electronico:</label>
